	<script>
		function updateTime() {
			let now = new Date();
			let hours = now.getHours().toString().padStart(2, '0');
			let minutes = now.getMinutes().toString().padStart(2, '0');
			let seconds = now.getSeconds().toString().padStart(2, '0');
			document.getElementsByClassName("time-part1")[0].textContent = `${hours}:${minutes}:${seconds}`;
			document.getElementsByClassName("time-part1")[1].textContent = `${hours}:${minutes}:${seconds}`;
			let days = ["Chủ Nhật", "Thứ Hai", "Thứ Ba", "Thứ Tư", "Thứ Năm", "Thứ Sáu", "Thứ Bảy"];
			let dayName = days[now.getDay()];
			let date = now.getDate().toString().padStart(2, '0');
			let month = (now.getMonth() + 1).toString().padStart(2, '0'); // Tháng bắt đầu từ 0 nên +1
			let year = now.getFullYear();
			document.getElementsByClassName("date-part1")[1].innerHTML = `${dayName}, ${date}/${month}/${year}`;
		}
		updateTime();
		setInterval(updateTime, 1000);

